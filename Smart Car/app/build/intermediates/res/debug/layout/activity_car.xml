<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".CarActivity" >

    <ImageButton
        android:id="@+id/pictureBtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button_white"
        android:src="@drawable/ic_take_picture"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"/>

    <ImageButton
        android:id="@+id/honkBtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button_white"
        android:src="@drawable/ic_action_honk"
        android:layout_above="@+id/goBackwardBtn"
        android:layout_toRightOf="@+id/goLeftBtn"
        android:layout_toEndOf="@+id/goLeftBtn"
        android:padding="3dp" />

    <ImageButton
        android:id="@+id/settingsBtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button_white"
        android:layout_alignParentRight="true"
        android:src="@drawable/ic_action_settings" />

    <ImageButton
        android:id="@+id/goForwardBtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button_white"
        android:src="@drawable/ic_go_forward"
        android:layout_above="@+id/goRightBtn"
        android:layout_toLeftOf="@+id/goRightBtn"
        android:padding="3dp" />

    <ImageButton
        android:id="@+id/goBackwardBtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button_white"
        android:src="@drawable/ic_go_backward"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/goRightBtn"
        android:padding="3dp" />

    <ImageButton
        android:id="@+id/goRightBtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button_white"
        android:src="@drawable/ic_go_right"
        android:layout_above="@+id/goBackwardBtn"
        android:layout_alignParentRight="true"
        android:padding="3dp" />

    <ImageButton
        android:id="@+id/goLeftBtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button_white"
        android:src="@drawable/ic_go_left"
        android:layout_alignParentLeft="false"
        android:layout_alignParentBottom="false"
        android:layout_toLeftOf="@+id/goForwardBtn"
        android:layout_above="@+id/goBackwardBtn"
        android:padding="3dp" />

    <ImageView
        android:id="@+id/sensView"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/denied_32x32"
        android:layout_alignRight="@+id/goRightBtn"
        android:layout_above="@+id/goForwardBtn" />

    <ImageButton
        android:id="@+id/ytLeftBtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button_white"
        android:src="@drawable/ic_go_left"
        android:layout_alignTop="@+id/goLeftBtn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:padding="3dp" />

    <ImageButton
        android:id="@+id/ytDownBtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button_white"
        android:src="@drawable/ic_go_backward"
        android:layout_toRightOf="@+id/ytLeftBtn"
        android:layout_toEndOf="@+id/ytLeftBtn"
        android:padding="3dp"
        android:layout_alignParentBottom="true" />

    <ImageButton
        android:id="@+id/ytUpBtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button_white"
        android:src="@drawable/ic_go_forward"
        android:layout_above="@+id/ytRightBtn"
        android:layout_toLeftOf="@+id/ytRightBtn"
        android:layout_toStartOf="@+id/ytRightBtn"
        android:padding="3dp" />

    <ImageButton
        android:id="@+id/ytRightBtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button_white"
        android:src="@drawable/ic_go_right"
        android:layout_above="@+id/ytDownBtn"
        android:layout_toRightOf="@+id/ytDownBtn"
        android:layout_toEndOf="@+id/ytDownBtn"
        android:padding="3dp" />

    <ImageButton
        android:id="@+id/ytCentBtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button_white"
        android:src="@drawable/ic_action_refresh"
        android:layout_above="@+id/ytDownBtn"
        android:layout_toRightOf="@+id/ytLeftBtn"
        android:layout_toEndOf="@+id/ytLeftBtn"
        android:padding="3dp" />

    <ImageButton
        android:id="@+id/radarBtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button_white"
        android:src="@drawable/rss_alt_32x32"
        android:layout_toEndOf="@+id/ytRightBtn"
        android:padding="3dp"
        android:layout_alignLeft="@+id/ytLeftBtn"
        android:layout_above="@+id/ytUpBtn" />

    <ImageButton
        android:id="@+id/lightBtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button_white"
        android:src="@drawable/sun_alt_fill_32x32"
        android:layout_toEndOf="@+id/radarBtn"
        android:padding="3dp"
        android:layout_alignLeft="@+id/ytLeftBtn"
        android:layout_above="@+id/radarBtn"
        android:contentDescription="" />

</RelativeLayout>
